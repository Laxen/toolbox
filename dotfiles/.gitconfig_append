# The configs here will be appended to your .gitconfig when you install the toolbox
[alias]
    st = status
    ci = commit
    co = checkout
    br = branch
    dt = difftool
    cp = cherry-pick
    at = branch -u origin/master
    as = rebase -i --autosquash
    cif = commit --fixup
    track = branch --set-upstream-to
    ls = log --all --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(auto)%d%C(reset)'
